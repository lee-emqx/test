objects= main.o 
namomq:$(objects)
	gcc -o nanomq $(objects)
	rm -f *.o nanomq
main.o:main.c handle_connect.c  
	gcc -o main.o main.c 
handle_connect.o:handle_connect.c mosquitto_base.h  
	gcc -o handle_connect.o handle_connect.c 
send_connack.o:send_connack.c 
	gcc -o send_connack.o send_connack.c
mosquitto_base.o:mosquitto_base.c 
	gcc -o mosquitto_base.o mosquitto_base.c
	
.PHONY:clean
clean:
	rm -f *.o nanomq
